<p id="notice"><%= notice %></p>

<h1>Listing User Role Relationships</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_role_relationships.each do |user_role_relationship| %>
      <tr>
        <td><%= user_role_relationship.user_id %></td>
        <td><%= user_role_relationship.role_id %></td>
        <td><%= link_to 'Show', user_role_relationship %></td>
        <td><%= link_to 'Edit', edit_user_role_relationship_path(user_role_relationship) %></td>
        <td><%= link_to 'Destroy', user_role_relationship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User role relationship', new_user_role_relationship_path %>
